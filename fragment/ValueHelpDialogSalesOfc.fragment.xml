<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Sales Office"
		ok=".onValueHelpSalesOfcOkPress"
		cancel=".onValueHelpSalesOfcCancelPress"
		supportRanges="true"
		key="VKBUR"
		descriptionKey="BEZEI"
        supportMultiselect="false">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearchSalesOfc" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="_BASIC"
						name="VKBUR"
						label="Sales Office"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="VKBUR" />
						</fb:control>
					</fb:FilterGroupItem>
                    	<fb:FilterGroupItem
						groupName="_BASIC"
						name="BEZEI"
						label="Description"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="BEZEI" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
